package com.project.sitefilmes.model.dto;

public class MovieDTO {

	private Long id;

	private String title;

	private String overview;

	private String releaseDate;

	private String posterPath;

	private String idMovie;

	public MovieDTO() {
		super();
	}

	public MovieDTO(String title, String overview, String releaseDate, String posterPath, String idMovie) {
		super();
		this.title = title;
		this.overview = overview;
		this.releaseDate = releaseDate;
		this.posterPath = posterPath;
		this.idMovie = idMovie;
	}

	public MovieDTO(Long id, String title, String overview, String releaseDate, String posterPath, String idMovie) {
		super();
		this.id = id;
		this.title = title;
		this.overview = overview;
		this.releaseDate = releaseDate;
		this.posterPath = posterPath;
		this.idMovie = idMovie;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getOverview() {
		return overview;
	}

	public void setOverview(String overview) {
		this.overview = overview;
	}

	public String getReleaseDate() {
		return releaseDate;
	}

	public void setReleaseDate(String releaseDate) {
		this.releaseDate = releaseDate;
	}

	public String getPosterPath() {
		return posterPath;
	}

	public void setPosterPath(String posterPath) {
		this.posterPath = posterPath;
	}

	public String getIdMovie() {
		return idMovie;
	}

	public void setIdMovie(String idMovie) {
		this.idMovie = idMovie;
	}

}
